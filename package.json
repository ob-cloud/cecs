{
  "name": "smart",
  "version": "0.1.0",
  "description": "OB Smart Management Platform",
  "author": "eamiear <justcall_me_k@163.com>",
  "scripts": {
    "serve": "vue-cli-service serve --fix",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "release": "yarn build & release.sh"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "echarts": "^4.4.0",
    "element-ui": "^2.5.4",
    "md5": "^2.2.1",
    "messageformat": "^2.3.0",
    "nprogress": "^0.2.0",
    "vue": "^2.5.22",
    "vue-echarts": "^4.0.4",
    "vue-i18n": "^8.15.0",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-utopa": "1.0.5",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "utopa"
    ],
    "rules": {
      "array-callback-return": "off",
      "no-nested-ternary": "off",
      "arrow-body-style": "off",
      "object-shorthand": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
